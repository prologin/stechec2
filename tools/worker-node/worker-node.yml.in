# Informations to connect to the master server.
master:
  host: master-node
  port: 8067
  heartbeat_secs: 5             # Must be < to the master timeout.
  shared_secret: <undefined>    # Must match the one in the master config.

# Configuration of this worker.
worker:
  port: 8068                    # Port used for RPC communication.
  available_slots: 20           # Number of parallel tasks that can be executed
                                # on this worker.
  port_range_start: 40000       # Port range used for tasks that need to listen
                                # on ports (match server for example).
  port_range_end: 41000

# Contest specific informations.
contest:
  dumper: /usr/lib/CHANGEME.so  # Library to be used to dump replays.
  directory: CHANGEME           # Shared directory. Must point to the same data
                                # for all workers and the website.
